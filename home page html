        <script type="text/javascript">
            $(document).ready(function() {
                $('.slider1').flexslider({
                    animation: "slide",
                    animationLoop: false,
                    itemWidth: 250,
                    itemMargin: 0
                });
                $('.dropdown-menu').click(function() {
                    $(this).find('.nav-link').slideToggle('slow');
                });
                $('.nav-link li a').click(function(e) {
                    e.stopPropagation();
                });
                $('.bxslider').bxSlider({
                    auto: true,
                    autoControls: false,
                    pager: true
                });
                $(window).scroll(function() {
                    if ($(this).scrollTop()) {
                        $('#top').fadeIn();
                    } else {
                        $('#top').fadeOut();
                    }
                });
                $(".icon-up-2").click(function() {
                    $("html, body").animate({scrollTop: 0}, 1000);
                });
            });
        jQuery(document).ready(function() {
                if (jQuery.browser.webkit) {
                    jQuery('body').addClass('Chrome');
                }
            });
        </script>
